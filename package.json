{
  "name": "bgg-sdk",
  "version": "0.5.2",
  "description": "Node SDK for querying the Board Game Geek (BGG) XML2 API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColCross/bgg-node-sdk.git"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest --passWithNoTests",
    "prepare": "husky",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "bgg",
    "boardgamegeek",
    "xmlapi2",
    "xml",
    "json"
  ],
  "author": "Colten Cross",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "xml-js": "^1.6.11"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "*.ts": "npm run lint"
  }
}
